{
	"name": "ibt",
	"version": "0.0.1",
	"description": "An in basket test for 2019",
	"main": "index.js",
	"scripts": {
		"test": "jest",
		"start": "webpack-dev-server --env dev",
		"release": "webpack --env prd",
		"gherkin": "./node_modules/.bin/codeceptjs gherkin:snippets",
		"test-acceptance": "./node_modules/.bin/codeceptjs run",
		"test-all": "./node_modules/.bin/codeceptjs run && jest"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@babel/polyfill": "^7.4.4",
		"@types/lodash": "^4.14.134",
		"babel-cli": "^6.26.0",
		"babel-preset-es2015": "^6.24.1",
		"lodash": "^4.17.11",
		"node-sass": "^4.12.0",
		"react": "^16.8.6",
		"react-app-polyfill": "^0.2.2",
		"react-dom": "^16.8.6"
	},
	"jest": {
		"transform": {
			".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
		},
		"testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js"
		],
		"testPathIgnorePatterns": [
			"/node_modules/",
			"/.git/"
		],
		"moduleNameMapper": {
			"\\.(css|less)$": "identity-obj-proxy",
			"@ibt-root(.*)$": "<rootDir>/src/$1",
			"@ibt-components": "<rootDir>/src/components",
			"@ibt-models": "<rootDir>/src/models"
		}
	},
	"devDependencies": {
		"@babel/preset-es2015": "^7.0.0-beta.53",
		"@types/enzyme": "^3.9.3",
		"@types/jest": "^23.3.14",
		"@types/react": "^16.8.20",
		"@types/react-dom": "^16.8.4",
		"awesome-typescript-loader": "^5.2.0",
		"babel-core": "^6.26.3",
		"babel-loader": "^7.1.4",
		"babel-preset-env": "^1.7.0",
		"codeceptjs": "^1.4.6",
		"css-loader": "^0.28.11",
		"enzyme": "^3.10.0",
		"enzyme-adapter-react-16": "^1.14.0",
		"html-webpack-plugin": "^3.2.0",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^23.2.0",
		"jest-css-modules": "^1.1.0",
		"react-test-renderer": "^16.8.6",
		"sass-loader": "^7.1.0",
		"source-map-loader": "^0.2.3",
		"style-loader": "^0.21.0",
		"ts-jest": "^22.4.6",
		"ts-loader": "^4.4.2",
		"typescript": "^2.9.2",
		"typings-for-css-modules-loader": "^1.7.0",
		"uglifyjs-webpack-plugin": "^2.1.3",
		"webpack": "^4.34.0",
		"webpack-cli": "^3.3.4",
		"webpack-dev-server": "^3.7.2",
		"webpack-merge": "^4.2.1"
	}
}
